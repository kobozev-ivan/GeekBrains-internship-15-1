<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.web.service.Pages" table="pages">
        <id name="ID" column="ID" type="java.lang.Integer"/>
        <property name="siteID" column="SITE_ID" type="java.lang.Integer"/>
        <property name="URL" column="URL" type="java.lang.String"/>
        <property name="found" column="FOUND" type="java.util.Date"/>
        <property name="lastScan" column="LAST_SCAN" type="java.util.Date"/>
    </class>
</hibernate-mapping>