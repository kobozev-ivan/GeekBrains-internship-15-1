<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.web.service.PersonPageRank" table="person_page_rank">
        <id name="ID" column="ID" type="java.lang.Integer"/>
        <property name="rank" column="RANK" type="java.lang.Integer"/>
        <set name="pageID">
            <key column="ID"></key>
            <one-to-many class="com.web.service.Pages"/>
        </set>
        <set name="personID">
            <key column="ID"></key>
            <one-to-many class="com.web.service.Persons"/>
        </set>
    </class>
</hibernate-mapping>