<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.web.service.hibernate.Pages" table="pages">
        <id name="ID" column="id" type="java.lang.Integer">
            <generator class="identity"/>
        </id>
        <property name="siteID" column="SITEID" type="java.lang.Integer"/>
        <property name="URL" column="URL" type="java.lang.String"/>
        <property name="Found" column="FOUND" type="java.lang.String"/>
        <property name="LastScan" column="LASTSCAN" type="java.lang.String"/>
    </class>
</hibernate-mapping>